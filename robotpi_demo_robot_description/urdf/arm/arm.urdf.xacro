<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
    <xacro:include filename="$(find robotpi_demo_robot_description)/urdf/ros2_control/demo_robot_gazebo_ros2_control.xacro"/>
    <xacro:include filename="$(find robotpi_demo_robot_description)/urdf/ros2_control/demo_robot_arm_ros2_control.xacro"/>

    <xacro:macro name="arm" params="name parent_link:=base_link *origin">

    <xacro:macro name="default_transmission" params="joint_name">
        <transmission name="transmission_${joint_name}">
            <plugin>transmission_interface/SimpleTransmission</plugin>
            <actuator name="actuator_${joint_name}" role="actuator_${joint_name}"/>
            <joint name="${joint_name}" role="${joint_name}">
                <mechanical_reduction>1.0</mechanical_reduction>
            </joint>
        </transmission>
    </xacro:macro>

    <xacro:property name="link_name" value="${name}"/>
    <xacro:property name="joint_name" value="${name}_joint"/>

    <xacro:property name="effort" value="87"/>
    <xacro:property name="velocity" value="2.792527"/>

    
